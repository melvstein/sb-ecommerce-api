					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const responseBody = pm.response.json();\r",
									"const accessToken = responseBody?.data?.accessToken || null;\r",
									"const refreshToken = responseBody?.data?.refreshToken || null;\r",
									"\r",
									"if (accessToken) {\r",
									"    pm.collectionVariables.set(\"accessToken\", accessToken);\r",
									"    console.log(\"Saved accessToken: \", accessToken)\r",
									"}\r",
									"\r",
									"if (refreshToken) {\r",
									"    pm.collectionVariables.set(\"refreshToken\", refreshToken);\r",
									"    console.log(\"Saved refreshToken: \", refreshToken)\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
